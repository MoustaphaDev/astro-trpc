{
    "name": "astro-trpc",
    "version": "0.2.0",
    "description": "Astro tRPC - End-to-end typesafe APIs in Astro wesbites made easy",
    "license": "MIT",
    "main": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf ./dist",
        "test": "vitest",
        "dev": "tsc-watch --onSuccess 'sh ./scripts/dev-script.sh'",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "api-client",
        "trpc",
        "astro",
        "astro-trpc"
    ],
    "author": "NoustaphaDev",
    "dependencies": {
        "@trpc/client": "^9.27.2",
        "@trpc/server": "^9.27.2",
        "astro": "^1.2.4"
    },
    "devDependencies": {
        "@types/node": "^18.7.13",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.8.2",
        "vitest": "^0.23.2"
    },
    "repository": {
        "type": "git",
        "url": ""
    }
}
